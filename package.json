{
  "name": "react-boilerplate",
  "version": "2.1.0",
  "description": "A boilerplate for React projects.",
  "author": "fairyscript",
  "license": "MIT",
  "keywords": [
    "react",
    "rsbuild",
    "typescript",
    "biome",
    "prettier",
    "husky",
    "lint-staged"
  ],
  "scripts": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "format": "prettier --write .",
    "preview": "rsbuild preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "biome check --write",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@rsbuild/core": "^1.6.8",
    "@rsbuild/plugin-react": "^1.4.2",
    "@swc/plugin-emotion": "^12.0.0",
    "@types/bun": "^1.3.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
