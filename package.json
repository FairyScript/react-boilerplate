{
  "name": "react-boilerplate",
  "version": "2.0.0",
  "description": "A boilerplate for React projects.",
  "author": "fairyscript",
  "license": "MIT",
  "keywords": [
    "react",
    "rsbuild",
    "typescript",
    "biome",
    "prettier",
    "husky",
    "lint-staged"
  ],
  "scripts": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "format": "prettier --write .",
    "preview": "rsbuild preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "biome check --write",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rsbuild/core": "^1.2.15",
    "@rsbuild/plugin-react": "^1.1.1",
    "@swc/plugin-emotion": "^7.0.3",
    "@types/bun": "^1.2.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  }
}